{
  "name": "thp-js",
  "version": "1.0.0",
  "description": "A basics javascript from TheHackingProject fullstack courses",
  "main": "hello.js",
  "scripts": {
    "devcopy:bsJavascript": "copyfiles -f ./node_modules/bootstrap/dist/js/bootstrap.bundle.min.* ./assets/js/bootstrap/",
    "devcopy:bsiconsCSS": "copyfiles -f ./node_modules/bootstrap-icons/font/bootstrap-icons.scss ./assets/fonts/",
    "devcopy:bsiconsWoff": "copyfiles -f ./node_modules/bootstrap-icons/font/fonts/** ./assets/fonts/",
    "watch:sass": "sass --watch ./assets/scss/bootstrap_customized.scss:./assets/css/boostrap_customized.css",
    "start": "npm-run-all devcopy:* --parallel watch:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WilfriedPaillot/THP-TP-BasicsJavascripts.git"
  },
  "keywords": [
    "TheHackingProject",
    "Validating project",
    "Javascript"
  ],
  "author": "Wilfried PAILLOT",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.1",
    "copyfiles": "^2.4.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.44.0"
  },
  "bugs": {
    "url": "https://github.com/WilfriedPaillot/THP-TP-BasicsJavascripts/issues"
  },
  "homepage": "https://github.com/WilfriedPaillot/THP-TP-BasicsJavascripts#readme"
}
